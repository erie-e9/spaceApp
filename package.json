{
  "name": "spaceApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --fix . --c .js,.jsx,.ts,.tsx,",
    "start": "npx react-native start --reset-cache",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "androidbuild": "react-native run-android --variant=release --verbose",
    "android": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd android && ./gradlew clean && ./gradlew --stop && rm -rf android/app/build android/app/.cxx android/build && cd .. && npx react-native start --reset-cache",
    "run:a": "yarn react-native run-android --active-arch-only",
    "ios": "rm -rf /Users/erictorresandrade/Caches/CocoaPods && rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd ios && pod deintegrate && pod setup && pod cache clean --all && rm -rf Pods/ build/ Podfile.lock && bundle install && pod install && cd .. && npx react-native start --reset-cache",
    "reload": "yarn && rm -rf android/app/build android/app/.cxx android/build && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx pod-install",
    "aabi": "cd android && adb uninstall com.spaceApp && cd app && rm -rf .cxx && cd .. && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native run-android --active-arch-only",
    "analyze:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./android-release.bundle --sourcemap-output ./android-release.bundle.map --reset-cache",
    "analyze:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios-release.bundle --sourcemap-output ./ios-release.bundle.map --reset-cache",
    "check-dependencies": "npx rnx-align-deps",
    "fix-dependencies": "npx rnx-align-deps --write"
  },
